<div class="container-fluid login-page">
<h1>Log In</h1>
<%= form_tag sessions_path do %>
    <%= text_field_tag :email, params[:email], placeholder: 'Email', required: true %>
    <br>
    <%= password_field_tag :password, "", placeholder: 'Password', required: true %>
    <br><br>
    <%= submit_tag 'Log In' %>
<% end %>
</div>